{
  "name": "uizoo",
  "version": "1.3.0",
  "description": "Dynamic React Component Library",
  "scripts": {
    "start": "npm run gulp:w",
    "build": "npm run build:iife && npm run build:cjs && npm run build:es && npm run build:min",
    "build:iife": "./node_modules/.bin/rollup -c rollup.config.js -o dist/index.js -f iife -n UiZoo",
    "build:cjs": "./node_modules/.bin/rollup -c rollup.config.js -o dist/index.cjs.js -f cjs -n UiZoo",
    "build:es": "./node_modules/.bin/rollup -c rollup.config.js -o dist/index.es.js -f es -n UiZoo",
    "build:min": "npm run build:min:js && npm run build:min:css",
    "build:min:js": "./node_modules/.bin/uglifyjs dist/index.js -o dist/index.min.js -c -m",
    "build:min:css": "./node_modules/.bin/cleancss dist/index.css -o dist/index.min.css",
    "gulp:w": "./node_modules/.bin/gulp",
    "test": "npm start & SERVER_PID=$!; (cd tests; npm i && node run-tests.js); kill $SERVER_PID"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myheritage/uizoo.js.git"
  },
  "keywords": [
    "dynamic",
    "react",
    "library",
    "component"
  ],
  "author": "MyHeritage",
  "bugs": {
    "url": "https://github.com/myheritage/uizoo.js/issues"
  },
  "homepage": "https://github.com/myheritage/uizoo.js#readme",
  "bin": {
    "uizoo": "./bin/uizoo.js"
  },
  "dependencies": {
    "babel-standalone": "~6.25.0",
    "chalk": "~1.1.3",
    "doctrine-standalone": "~1.0.0",
    "fs-extra": "~4.0.2",
    "latest-version": "~3.1.0",
    "opn": "~5.1.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-router-dom": "~4.2.2",
    "underscore": "~1.8.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "body-parser": "~1.15.2",
    "clean-css-cli": "^4.1.10",
    "compression": "~1.6.2",
    "cors": "~2.8.3",
    "express": "~4.15.2",
    "glob": "7.0.6",
    "gulp": "3.9.1",
    "gulp-livereload": "~3.8.1",
    "gulp-nodemon": "~2.2.1",
    "postcss": "~5.2.6",
    "postcss-cssnext": "~2.9.0",
    "postcss-discard-comments": "~2.0.4",
    "postcss-discard-duplicates": "~2.0.2",
    "rollup": "~0.50.0",
    "rollup-plugin-buble": "~0.16.0",
    "rollup-plugin-commonjs": "~8.2.1",
    "rollup-plugin-json": "~2.3.0",
    "rollup-plugin-node-resolve": "~3.0.0",
    "rollup-plugin-replace": "~2.0.0",
    "rollup-plugin-scss": "~0.3.0",
    "uglify-js": "~3.1.6"
  }
}
